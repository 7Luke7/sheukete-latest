<Map srs="+init=epsg:4326">
  <Layer name="Lines" srs="+init=epsg:4326">
    <StyleName>LineStyle</StyleName>
    <Datasource>
      <Parameter name="type">postgis</Parameter>
      <Parameter name="host">127.0.0.1</Parameter>
      <Parameter name="port">5432</Parameter>
      <Parameter name="dbname">georgia_osm</Parameter>
      <Parameter name="user">postgres</Parameter>
      <Parameter name="password">2007luka2007</Parameter>
      <Parameter name="table">(SELECT ST_AsGeoJSON(geom) AS geojson FROM get_vector_tile(5, 12, 18)) AS data</Parameter>
      <Parameter name="geometry_field">geom</Parameter>
      <Parameter name="srid">4326</Parameter>
    </Datasource>
  </Layer>

<Style name="LineStyle">
    <Rule>
      <LineSymbolizer stroke="#0000FF" stroke-width="2" stroke-opacity="1"/>
    </Rule>
  </Style>
</Map>